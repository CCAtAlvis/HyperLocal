CREATE TABLE `hyperlocal`.`login` ( `id` INT NOT NULL AUTO_INCREMENT ,  `username` VARCHAR(30) NOT NULL ,  `email` VARCHAR(100) NOT NULL ,  `password` CHAR(60) NOT NULL ,  `confcode` CHAR(32) NOT NULL ,  `created_on` DATETIME NOT NULL ,    PRIMARY KEY  (`id`),    UNIQUE  `username` (`username`),    UNIQUE  `email` (`email`),    UNIQUE  `confcode` (`confcode`)) ENGINE = InnoDB;